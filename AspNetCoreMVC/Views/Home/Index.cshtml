@model IEnumerable<AspNetCoreMVC.Models.EmployeeView>
@{
   ViewBag.Title = "Home";
}

<table class="table table-dark table-striped">
   @foreach (var item in Model)
   {
      <tr>
         <td>Имя: @item.Name</td>
         <td>Фамилия: @item.Surname</td>
         <td><a asp-action="ViewModel" asp-route-id="@item.id">Подробно</a> </td>
      </tr>
   }
</table>

